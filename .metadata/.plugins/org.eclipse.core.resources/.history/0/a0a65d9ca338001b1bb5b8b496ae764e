package com.logic;

public class SecondMinwithDupl {
	public static void main(String[] args)
	{
	int a[]= {23,56,24,98,67,52,23,24,55,56,78,10};
	int smallfirst=0;
	int smallsecond=0;
	
	for (int i = 0; i < a.length; i++) 
	{
		if(a[i]<smallfirst)
		{
			smallsecond=smallfirst;
			smallfirst=a[i];
			
		}
		else if (a[i]<smallsecond && a[i]!=smallfirst)
		{
			smallsecond=a[i];
		}
	}
	System.out.println(smallsecond);
	}
}
