package com.catDog;

public class CatDog 

{
	
	public boolean catdog(String str)
	{
		int count=0;
		for (int i = 0; i < str.length()-2; i++) 
		{
			if(str.substring(i, i+3).equals("cat"))
			{
				count++;
			}
			else if (str.substring(i, i+3).equals("dog")) 
			{
			count--;	
			}
			
			if(count==0)
			{
				return true;
			}
			
				return false;
			
		}
	}
public static void main(String[] args)
{
	
	
}

}
