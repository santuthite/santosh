package com.logic;

public class ThirdLargestwithDupl {
public static void main(String[] args) {
	int a[]= {90,10,90,30,60,90,40,10,70,80,50,90,80,10,20};
	int third=0,second=0,first=0;
	
	for (int i = 0; i < a.length; i++) 
	{
		if(a[i]>first)
		{
			third=second;
			second=first;
			first=a[i];
		}
		else if (a[i]>second && a[i]!=first)
		{
			third=second;
			second=a[i];
		}
		else if (a[i]>third && a[i]>second && a[i]!=0)
		{
			third=a[i];
		}
	}
	System.out.println(third);
}
}
